{
  "name": "Sample Plugin, For Testing",
  "uniqueId": "sample_plugin",
  "description": "This plugin just takes in the inputs provided, and prints them out. Use it as a template to see what parameters are available to a script.",
  "plugin-format": "python",
  "type": "trainer",
  "version": "1.0.4",
  "model_architectures": ["LlamaForCausalLM"],
  "supported_hardware_architectures": ["cpu", "cuda", "mlx"],
  "files": ["main.py", "setup.sh"],
  "setup-script": "setup.sh",
  "parameters": {
    "adaptor_name": {
      "title": "Adaptor Name",
      "type": "string",
      "required": true
    },
    "model": {
      "title": "Model",
      "type": "string",
      "default": "EleutherAI/gpt-neo-2.7B",
      "required": "true"
    },
    "model_type": {
      "title": "Model Type",
      "type": "string",
      "default": "hf-causal",
      "required": "true"
    }
  }
}
